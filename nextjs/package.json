{
  "name": "@se-2/nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "codegen": "run-p codegen:typechain codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:typechain": "typechain --target ethers-v5 --out-dir src/generated/rollups './node_modules/@cartesi/rollups/export/artifacts/**/*.json'",
    "build": "next build",
    "serve": "next start",
    "lint": "next lint",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "check-types": "tsc --noEmit --incremental",
    "vercel": "vercel",
    "vercel:yolo": "vercel --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true"
  },
  "dependencies": {
    "@cartesi/rollups": "1.0.0",
    "@ethersproject/providers": "^5.7.2",
    "@heroicons/react": "^2.0.11",
    "@particle-network/auth": "^1.3.1",
    "@particle-network/chains": "^1.3.21",
    "@particle-network/connect": "^1.2.1",
    "@particle-network/connect-react-ui": "^1.2.1",
    "@rainbow-me/rainbowkit": "1.3.5",
    "@uniswap/sdk-core": "^4.0.1",
    "@uniswap/v2-sdk": "^3.0.1",
    "axios": "^1.7.2",
    "blo": "^1.0.1",
    "daisyui": "4.5.0",
    "ethers": "^5.7.2",
    "form-data": "^4.0.0",
    "graphql": "^16.9.0",
    "next": "^14.0.4",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-toastify": "^10.0.5",
    "urql": "^2.2.0",
    "use-debounce": "^8.0.4",
    "usehooks-ts": "^2.13.0",
    "viem": "1.19.9",
    "wagmi": "1.4.12",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/node": "^17.0.35",
    "@types/nprogress": "^0",
    "@types/react": "^18.0.9",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.15.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "type-fest": "^4.6.0",
    "typechain": "^8.3.2",
    "typescript": "^5.1.6",
    "vercel": "^32.4.1"
  }
